[Unit]
Description=D9 Node
After=network-online.target

[Service]
User=ubuntu  
ExecStart=/home/ubuntu/dev-alice/target/release/d9-node --base-path /home/ubuntu/alice/chain-data --chain /home/ubuntu/alice/burn-staking-spec.json --port 40100 --alice
Restart=on-failure
RestartSec=3
LimitNOFILE=4096

[Install]
WantedBy=multi-user.target
